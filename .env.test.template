# Copy this file to `.env` for local test runs.
# This file is not loaded automatically unless renamed to `.env`.

DISCORD_MODE=test
DISCORD_ALLOW_PROD=0

DISCORD_TEST_GUILD_ID=
DISCORD_TEST_NOTIFY_CHANNEL_ID=
DISCORD_TEST_ARCHIVE_CHANNEL_ID=
DISCORD_ACCEPTED_ARCHIVE_DELAY_MINUTES=30

DISCORD_ALLOWED_ROLE_NAMES=RRO,RRO ICs
